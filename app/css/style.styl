@import 'rupture'
@import "variable/_function.styl"
@import "variable/_ie.styl"
@import "icon/sprite.css"
@import "variable/_fonts.styl"
@import "variable/_helper.styl"
@import "variable/_default.styl"
@import "variable/_elements.styl"
@import "variable/_constant.styl"
//@import "variable/_grid.styl"

rupture.scale = 0 480px 768px 992px 1200px
rupture.scale-names = 'xs' 'sm' 'md' 'lg'

// Grid breakpoints
$breakpoints = {
	'xs' :  0,
	'sm' : 480px,
	'md' : 768px,
	'lg' : 992px,
	'xl' : 1200px
}

// Number of columns
$columns = 12

init($breakpoints, $columns)

body
	+below(768px)
		//padding-top: 58px
	@extend .type--main
	font-family $v-font
	color $main-text-color
	line-height 1.2
	letter-spacing 0.01em
	width: 100%

.main-cont
	center(1200px,10px)

.page
	+below(1024px)
		display: block
		padding-top: 58px
	display flex
	min-height 100vh

.content
	+below(1024px)
		width: 100%
	flex-grow: 1
	flex-shrink: 1
	flex-basis: 0%
	width: calc(100% - 300px - 95px)
	max-width 1440px
	overflow hidden
	position: relative

.main-page
	width: 100%

.main-page__first-section
	f()
	fw()


.main-page__first-section-about
	+below(768px)
		width: 100%
	width: 50%

.main-page__first-section-slider
	.owl-dots
		width 100%
		flex(mid)
		position: absolute
		z-index 20
		bottom 10%
	.owl-dot
		round()
		margin 5px
		width 12px
		height 12px
		background #949190
	.owl-dot.active
		background none
		size 14px
		border 1px solid white
		position: relative
		&:before
			content ''
			round()
			background: white
			position: absolute
			size 8px
			top 2px
			left 2px

.main-page__first-section-info
	+below(768px)
		width: 100%
		f()
		fw()
		fl()
	width: 50%
	.item-block
		+below(768px)
			order 2
			width: 100%
		height: 50vh
		min-height 300px
		order 1
	.item-slider
		+below(768px)
			order 1
		order 2
.main-page__first-section-slider__el
	+below(768px)
		height: auto
		height calc(100vh - 105px)
		min-height 400px
		background-position: top center
	position: relative
	bg-full()
	//background-size:contain
	width: 100%
	min-height 600px
	height: 100vh

.filter
